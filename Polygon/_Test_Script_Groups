<#assign rand = "gen_rand"/>
<#function div a b>
    <#return (a / b)?int>
</#function>
<#function min a b>
    <#return [a, b]?sort?first>
</#function>
<#function max a b>
    <#return [a, b]?sort?last>
</#function>
<#function pow base power>
    <#assign res = 1/>
    <#list 1..power as it>
        <#assign res = res * base/>
    </#list>
    <#return res>
</#function>
<#function fact n>
    <#assign res = 1/>
    <#list 1..n as it>
        <#assign res = res * it/>
    </#list>
    <#return res>
</#function>
<#assign minN = 1/>
<#assign infN = pow(10, 6)/>
<#assign maxNs = [3, 100, pow(10, 4), infN]/>
<#macro script0 group>
    <#assign desc = "samples"/>
</#macro>
<#macro script1 group>
    <#assign desc = "n <= 100"/>
</#macro>
<#macro script2 group>
    <#assign desc = "n <= 10**4"/>
</#macro>
<#macro script3 group>
    <#assign desc = "full"/>    
</#macro>
<#assign scripts = [script0, script1, script2, script3]/>
<#assign groupsCount = scripts?size/>
<#list 0..<groupsCount as group_index>
    <#assign script = scripts[group_index]/>
    <@script group = group_index/>
</#list>
